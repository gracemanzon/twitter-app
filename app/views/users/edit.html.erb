<h1>Edit User</h1>
<%= form_with model: @user do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div>
  <%= form.label :image_url %>
  <%= form.text_field :image_url %>
</div>
 
  <div>
    <%= form.submit %>
  </div>
<% end%>