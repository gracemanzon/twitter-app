<h1>All Tweets</h1>
<% @tweets.each do |tweet| %>
  <h2><%= tweet.title %></h2>
  <p> <%= tweet.body %></p>
  <%= link_to "Delete tweet", tweet_path(tweet), data: {turbo_method: :delete} %>
  <div>
  <%= link_to "Edit tweet", edit_tweet_path(tweet) %>
  </div>
<% end %>

<div>
<%= link_to "New tweet", new_tweet_path %>
</div>